<AnchorPane prefHeight="172.0" prefWidth="107.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.thermometerView.ThermometerViewController">
   <children>
      <GridPane layoutY="2.0" prefHeight="240.0" prefWidth="304.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
