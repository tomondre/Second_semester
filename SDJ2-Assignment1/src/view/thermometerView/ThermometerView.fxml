          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="External Thermometer:" />
            <Label fx:id="t0Label" text="Label" GridPane.columnIndex="1" />
