<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="182.0" prefWidth="134.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.historyView.HistoryViewController">
   <children>
      <VBox prefHeight="329.0" prefWidth="259.0">
         <children>
            <Label fx:id="thermHistoryLabel" text="Label">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="historyTable" prefHeight="222.0" prefWidth="0.0">
              <columns>
                <TableColumn fx:id="indexesColumn" editable="false" prefWidth="91.0" text="#" />
                  <TableColumn fx:id="tempColumn" minWidth="0.0" prefWidth="155.0" text="Temperature" />
              </columns>
            </TableView>
            <Button fx:id="backButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onBackButton" text="Back" />
            <Button fx:id="graphButton" mnemonicParsing="false" onAction="#onGraphButton" text="See Graph" />
         </children>
      </VBox>
   </children>
</AnchorPane>
